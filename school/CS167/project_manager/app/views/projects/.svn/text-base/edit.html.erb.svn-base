<h1>Editing project</h1>

<% form_for(@project) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :abstract %><br />
    <%= f.text_area :abstract %>
  </p>
  <p>
    <%= f.label :milestone_id %><br />
    <%= f.text_field :milestone_id %>
  </p>
  <p>
    <%= f.label :data_sources %><br />
    <%= f.text_area :data_sources %>
  </p>
  <p>
    <%= f.label :references %><br />
    <%= f.text_area :references %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @project %> |
<%= link_to 'Back', projects_path %>
